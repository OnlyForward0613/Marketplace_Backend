{
  "name": "ink-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn build && yarn prisma:migrate:deploy && node ./dist/src/main",
    "start:db:dev": "yarn prisma:generate && yarn prisma:migrate:dev",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:dev": "prisma migrate dev --name autoMigrate --preview-feature",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:resolve": "prisma migrate resolve --applied",
    "prisma:db": "npx prisma db push --preview-feature",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "semantic-release",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@metamask/eth-sig-util": "6.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/swagger": "^7.1.11",
    "@openzeppelin/contracts": "^4.9.3",
    "@prisma/client": "^5.3.0",
    "argon2": "^0.31.1",
    "abi-decoder": "^2.4.0",
    "aws-sdk": "2.1444.0",
    "axios": "^1.5.0",
    "axios-retry": "^3.8.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "connect-redis": "7.1.0",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "deepmerge": "4.3.1",
    "dotenv": "16.3.1",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "5.7.2",
    "express": "4.18.2",
    "express-rate-limit": "6.9.0",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.1",
    "lodash": "4.17.21",
    "merkletreejs": "^0.3.10",
    "multer": "1.4.5-lts.1",
    "nanoid": "3.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "1.0.0",
    "pino": "8.15.0",
    "prisma": "^5.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "slugify": "1.6.6",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "validator": "13.11.0",
    "viem": "^1.10.14",
    "web3": "4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.17",
    "@types/ioredis": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/multer": "1.4.7",
    "@types/node": "20.3.1",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "commitizen": "4.3.0",
    "conventional-changelog-cli": "3.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "14.0.0",
    "pino-pretty": "10.2.0",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
